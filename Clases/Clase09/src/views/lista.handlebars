{{#if users.length}}

	{{#each users}}	{{! etiqueta apertura con # }}

		<p>Hola {{nombre}} {{apellido}} </p>
		<p>Tu correo es {{email}} y tu telefono es {{telefono}}</p>
		<p>Tu edad es {{edad}}</p>

	{{/each}}	{{! etiqueta de cierre con / }}

{{else}}
	<h2>No hay usuarios registrados</h2>

{{/if}}